.portlet {
	margin: 0px 0px 0.5em; padding: 0px; border: currentColor; =
border-image: none; width: 11.6em; overflow: hidden; float: none;
}
.portlet h3 {
	background: none; padding: 0px 1em 0px 0.5em; height: 1em; =
text-transform: lowercase; font-size: 91%; font-weight: normal; display: =
inline; white-space: nowrap;
}
.pBody {
	padding: 0px 0.8em 0.3em 0.5em; border: 1px solid rgb(170, 170, 170); =
border-image: none; color: black; font-size: 95%; border-collapse: =
collapse; background-color: white;
}
.portlet ul {
	line-height: 1.5em; font-size: 95%;
}
.portlet li {
	margin: 0px; padding: 0px;
}
#p-logo {
	left: 0px; top: 0px; width: 12em; height: 155px; overflow: visible; =
position: absolute; z-index: 3;
}
#p-logo h3 {
	display: none;
}
#p-logo a {
	background-position: 35% 50% !important; width: 12.2em; height: 155px; =
text-decoration: none; display: block; background-repeat: no-repeat;
}
#p-logo a:hover {
	background-position: 35% 50% !important; width: 12.2em; height: 155px; =
text-decoration: none; display: block; background-repeat: no-repeat;
}
#p-search {
	position: relative; z-index: 3;
}
